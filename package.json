{
  "name": "async-plugin-nunjucks",
  "version": "0.9.0",
  "description": "Async context properties for the nunjucks template engine",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geleto/async-plugin-nunjucks.git"
  },
  "keywords": [
    "nunjucks",
    "plugin",
    "async",
    "properties",
    "variables",
    "context"
  ],
  "author": "Angel Popov",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/geleto/async-plugin-nunjucks/issues"
  },
  "homepage": "https://github.com/geleto/async-plugin-nunjucks#readme",
  "devDependencies": {
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "nunjucks": "^3.0.0"
  }
}